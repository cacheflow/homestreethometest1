<%= form_for (@resident) do |f| %>
	Name <%= f.text_field :name %><br />
	Bio <%= f.text_field :bio %><br />
	Goals <%= f.text_field :goals %><br />
	Monetary Goal <%= f.text_field :goal_monetary %><br />
	<%= f.collection_select :partner_id, Partner.all, :id, :name %>
	<%= f.hidden_field :user_id, :value => current_user.id %><br />
	<%= f.submit %>

<% end %>